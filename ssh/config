#SSH setup


Host cybersim-mgmt
    HostName mgmt.boozallencybersim.com
    User cgloss
    ForwardAgent yes
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host cybersim-dev
    HostName mgmt.boozallencybersim.com
    User cgloss
    ForwardAgent yes
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

